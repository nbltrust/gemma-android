    <!-- 备份私钥页面 -->
    <style name="eos_text_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">@dimen/font_4</item>
        <item name="android:textColor">@color/black_title</item>
    </style>

    <style name="eos_text_body">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">@dimen/font_4</item>
        <item name="android:textColor">@color/black_context</item>
    </style>

    <!-- 配置钱包页面 -->
    <style name="eos_text_title_config_wallet">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_20</item>
        <item name="android:layout_marginStart">@dimen/dimen_12</item>
        <item name="android:layout_marginEnd">@dimen/dimen_12</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/font_3.5</item>
        <item name="android:textColor">@color/black_context</item>
        <item name="android:layout_marginTop">@dimen/dimen_6</item>
    </style>

    <style name="eos_editText_config_wallet">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_50</item>
        <item name="android:textColor">@color/black_context</item>
        <item name="android:textSize">@dimen/font_4</item>
        <item name="android:textColorHint">@color/very_light_pink</item>
        <item name="android:theme">@style/myEditText</item>
        <item name="android:background">@null</item>
        <item name="android:textCursorDrawable">@drawable/eos_cursor_custom</item>
        <item name="android:layout_marginEnd">@dimen/dimen_12</item>
        <item name="android:layout_marginStart">@dimen/dimen_12</item>
        <item name="android:textStyle">normal</item>

    </style>

    <!-- 钱包主页面 -->

    <style name="eos_show_cpu_status_area">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/eos_cpu_show_area_height</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="eos_cpu_net_ram">
        <item name="android:layout_width">@dimen/eos_single_cpu_width</item>
        <item name="android:layout_height">@dimen/eos_single_cpu_height</item>
        <item name="android:layout_marginBottom">@dimen/eos_single_cpu_margin</item>
        <item name="android:layout_marginTop">@dimen/eos_single_cpu_margin</item>
    </style>

    <style name="eos_textView_cpu_net_ram">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:layout_marginTop">@dimen/dimen_6</item>
        <item name="android:layout_marginStart">@dimen/dimen_15</item>
        <item name="android:layout_marginEnd">@dimen/dimen_20</item>
        <item name="android:textColor">@color/black_context</item>
        <item name="android:textSize">@dimen/font_3</item>
    </style>

    <style name="eos_progressBar_cpu_net_ram">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_3</item>
        <item name="android:layout_marginTop">@dimen/dimen_10</item>
        <item name="android:layout_marginStart">@dimen/dimen_15</item>
        <item name="android:layout_marginEnd">@dimen/dimen_20</item>
    </style>

    <!-- 管理钱包页面 -->
    <style name="eos_card_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/eos_card_height</item>
    </style>

</resources>
