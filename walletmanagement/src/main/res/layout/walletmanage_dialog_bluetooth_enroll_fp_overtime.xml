<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:title="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@drawable/shape_corner_dialog"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:src="@drawable/ic_overtime"
        android:id="@+id/iv_password_hint"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"/>

    <TextView
        android:id="@+id/tv_password_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/walletmanage_enroll_fp_overtime"
        android:textColor="@color/black_title"
        android:layout_gravity="center"
        android:textSize="@dimen/font_4"
        android:layout_marginTop="6dp"/>

    <View
        style="@style/common_divider"
        android:layout_marginTop="@dimen/dimen_20">

    </View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_jump"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="@string/walletmanage_jump"
            android:gravity="center"
            android:textSize="@dimen/font_4"
            android:layout_weight="1"
            android:textColor="@color/black_title"
            android:background="@drawable/shape_corner_left_bot"
            />

        <TextView
            android:id="@+id/tv_retry"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="@string/walletmanage_retry"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="@dimen/font_4"
            android:textColor="@color/black_title"
            android:background="@drawable/shape_corner_right_bot"
            />

    </LinearLayout>


</LinearLayout>