<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/view_root"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        layout="@layout/ui_navibar">

    </include>

    <!--<com.cybex.base.view.OverScrollView-->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_20"
                android:text="@string/walletmanage_tip_verify_mne"
                android:textAlignment="textStart"
                android:layout_marginLeft="@dimen/common_page_content_padding"
                android:layout_marginRight="@dimen/common_page_content_padding"
                android:textColor="@color/black_title"
                android:textSize="16dp"/>


            <com.cybex.walletmanagement.widget.LabelsView
                android:id="@+id/view_click_to_show_mne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/common_page_content_padding"
                android:background="@drawable/shape_corner_radius"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="28dp"
                android:paddingBottom="28dp">

            </com.cybex.walletmanagement.widget.LabelsView>


            <com.cybex.walletmanagement.widget.LabelsView
                android:id="@+id/view_show_mne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dimen_28"
                android:layout_marginStart="@dimen/dimen_28"
                android:layout_marginTop="10dp">

            </com.cybex.walletmanagement.widget.LabelsView>


        </LinearLayout>


    </ScrollView>


</LinearLayout>